    "#import\n",
    "\n",
    "from datetime import datetime\n",
    "import pandas as pd\n",
    "import pyspark.sql.functions as F\n",
    "import pyspark.sql.types as T"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "99b4f774",
   "metadata": {
    "ExecuteTime": {
     "end_time": "2022-10-31T14:17:10.420459Z",
     "start_time": "2022-10-31T14:17:10.416503Z"
    }
   },
   "outputs": [],
   "source": [
    "#pandas display settings\n",
    "\n",
    "pd.options.display.float_format = '{:,.2f}'.format\n",
    "pd.set_option('display.max_colwidth', None)\n",
    "pd.set_option('display.max_rows', None)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "1d62e39d",
   "metadata": {
    "ExecuteTime": {
     "end_time": "2022-10-31T14:17:10.583779Z",
     "start_time": "2022-10-31T14:17:10.580136Z"
    }
   },
   "outputs": [],
   "source": [
    "TODAY = datetime.today().strftime('%Y-%m-%d')\n",
    "HDFS_RUCIO_LOCKS    = \"/project/awg/cms/rucio/{}/locks/part*.avro\".format(TODAY)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 30,
   "id": "31b846bd",
   "metadata": {
    "ExecuteTime": {
     "end_time": "2022-10-31T14:35:10.313156Z",
     "start_time": "2022-10-31T14:35:06.624831Z"
    }
   },
   "outputs": [],
   "source": [
    "df_locks = spark.read.format('avro').load(HDFS_RUCIO_LOCKS) \\\n",
    "    .filter(F.col(\"STATE\") == 'O') \\\n",
    "    .withColumnRenamed(\"NAME\", \"file_name\") \\\n",
    "    .withColumn('file_size', F.col('BYTES').cast(T.LongType())) \\\n",
    "    .withColumn('rule', F.lower(F.hex(F.col('RULE_ID')))) \\\n",
    "    .withColumn('rse_id', F.lower(F.hex(F.col('RSE_ID')))) \\\n",
    "    .withColumn('created_date', F.to_date(F.from_unixtime(F.col('created_at')/1000)))\\\n",
    "    .select([\"file_name\", \"file_size\", \"rule\", \"rse_id\", \"account\", \"created_date\"])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 34,
   "id": "db7bb66c",
   "metadata": {
    "ExecuteTime": {
     "end_time": "2022-10-31T14:35:53.788592Z",
     "start_time": "2022-10-31T14:35:53.575927Z"
    }
   },
   "outputs": [],
   "source": [
    "wmcore_data_age = df_locks\\\n",
    "                    .filter(F.col(\"account\")==\"wmcore_output\")\\\n",
    "                    .withColumn(\"age_in_days\", F.datediff(F.current_date(), F.col(\"created_date\")))\\\n",
    "                    .groupBy(\"age_in_days\").sum(\"file_size\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 35,
   "id": "0302558b",
   "metadata": {
    "ExecuteTime": {
     "end_time": "2022-10-31T14:36:14.797395Z",
